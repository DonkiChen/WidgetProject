<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OrderStatusView">
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="osvRadius" format="dimension" />
        <attr name="osvRingRadius" format="dimension" />
        <attr name="osvLineWidth" format="dimension" />
        <attr name="osvCircleColor" format="color" />
        <attr name="osvLineColor" format="color" />
        <attr name="android:entries" />
        <attr name="osvCircleTitleSpacing" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SlideBar">
        <attr name="barNormalTextColor" format="color" />
        <attr name="barSelectedTextColor" format="color" />
        <attr name="barNormalTextSize" format="dimension" />
        <attr name="barSelectedTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LabelView">
        <attr name="labelGravity" format="enum">
            <enum name="topLeft" value="0" />
            <enum name="topRight" value="1" />
            <enum name="bottomLeft" value="2" />
            <enum name="bottomRight" value="3" />
        </attr>
        <attr name="labelBackgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="DualProgressView">
        <attr name="dpv_anim_duration" format="integer" />
        <attr name="dpv_outer_color" format="color" />
        <attr name="dpv_inner_color" format="color" />
        <attr name="dpv_thickness" format="dimension" />
        <attr name="dpv_inner_padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ProgressTextView">
        <attr name="ptvColor" format="color" />
        <attr name="ptvWidth" format="dimension" />
        <attr name="ptvFitTextHeight" format="boolean"/>
        <attr name="ptvIgnorePadding" format="boolean"/>
    </declare-styleable>
</resources>